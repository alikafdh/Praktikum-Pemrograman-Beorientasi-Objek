//deklarasi class
public class volumebalok {
    //deklarasi method utama
    public static void main(String[] args) {
        double volume;
        //instan objek dari class balok
        balok b1 = new balok();
        b1.panjang = 7; 
        b1.lebar = 4;
        b1.tinggi = 5;
        //hitung
        volume = b1.panjang*b1.lebar*b1.tinggi;
        System.out.println("Volume Balok = "+volume+"Cm3");
    }
}
